{
    "webroot": "wwwroot",
    "version": "1.0.0-*",

  "dependencies": {
    "Microsoft.AspNet.Mvc": "6.0.0-beta5",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-final",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta5",
    "Microsoft.AspNet.StaticFiles": "1.0.0-beta5"
  },

    "commands": {
        "web": "Microsoft.AspNet.Hosting --config hosting.ini",
    },

    "frameworks": {
        "dnx451": {
          "dependencies": {
            "IPC.Model": "1.0.0-*",
            "NLog": "4.2.3",
            "NLog.Config": "4.2.3"
          }
        },
        "dnxcore50": { }
    },

    "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components"
    ],
    "publishExclude": [
        "node_modules",
        "bower_components",
        "**.xproj",
        "**.user",
        "**.vspscc"
    ]
}
